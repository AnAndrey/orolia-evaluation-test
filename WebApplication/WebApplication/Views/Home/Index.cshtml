@using WebApplication.Controllers
@model WebApplication.Models.SettingsViewModel

@{
    ViewData["Title"] = "";
}

@section Scripts {
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/ng-file-upload-bower-12.2.13/ng-file-upload-shim.min.js"></script>
    <script src="~/lib/ng-file-upload-bower-12.2.13/ng-file-upload.min.js"></script>
    <script src="~/home/settings"></script>
    <script src="~/lib/ui-grid/ui-grid.js"></script>
    <script src="~/js/app-main.js"></script>
    <script src="~/js/filesController.js"></script>
    <script src="~/js/dataController.js"></script>
    <script src="~/js/uploadController.js"></script>
}

<div ng-app="app-main" id="wrapper-body">
    <div ng-controller="filesController" id="first-body">
        <div class="files-grid" ui-grid="gridOptions" ui-grid-cellNav  style="height: 500px"></div>
        <div ng-controller="uploadController">
            <input type="file" ngf-select="onFileSelect($files, gridOptions)" multiple>
        </div>
    </div>
    <div id="second-body">
        <my-elem></my-elem>
    </div>
</div>
